{
  "name": "react-webpack-template",
  "version": "1.0.0",
  "private": true,
  "description": "A React template bundled with Webpack 5 ",
  "main": "index.js",
  "scripts": {
    "dev": "set NODE_ENV=development&& webpack serve --config webpack.dev.js",
    "build-local": "set NODE_ENV=production&& webpack --config webpack.prod.js --env ANALYZE=true",
    "build": "set NODE_ENV=production&& webpack --config webpack.prod.js --env ANALYZE=false",
    "test": "set NODE_ENV=test&& jest --watchAll",
    "lint": "npx eslint src/**",
    "lint:fix": "npx eslint src/** --fix"
  },
  "author": "AlstonChan",
  "license": "MIT",
  "dependencies": {
    "@chan_alston/image": "^0.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.2",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "compression-webpack-plugin": "^10.0.0",
    "core-js": "^3.27.1",
    "css-loader": "^6.7.3",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.31.0",
    "eslint-plugin-react": "^7.32.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss": "^8.4.21",
    "postcss-assets": "^6.0.0",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^7.8.3",
    "react-refresh": "^0.14.0",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
