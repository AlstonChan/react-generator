{
  "name": "react-webpack-template",
  "version": "1.0.0",
  "private": true,
  "description": "A webpacked react application",
  "main": "index.js",
  "scripts": {
    "dev": "set NODE_ENV=development&& webpack serve --config webpack.dev.js",
    "build-local": "set NODE_ENV=production&& webpack --config webpack.prod.js --env ANALYZE=true",
    "build": "set NODE_ENV=production&& webpack --config webpack.prod.js --env ANALYZE=false",
    "test": "set NODE_ENV=test&& jest --watchAll"
  },
  "author": "AlstonChan",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "autoprefixer": "^10.4.11",
    "babel-jest": "^29.0.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "compression-webpack-plugin": "^10.0.0",
    "core-js": "^3.25.2",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^8.0.1",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.16",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.2",
    "react-refresh": "^0.14.0",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.54.9",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
