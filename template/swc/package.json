{
  "name": "react-generator",
  "version": "1.0.0",
  "private": true,
  "description": "A React app generator bundled with Webpack 5 and Babel",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config webpack.dev.js",
    "build:local": "cross-env NODE_ENV=production webpack --config webpack.prod.js --env ANALYZE=true",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js --env ANALYZE=false",
    "lint": "eslint src/**",
    "lint:fix": "eslint src/** --fix"
  },
  "dependencies": {
    "@chan_alston/image": "^1.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.14.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@swc/core": "^1.6.5",
    "@testing-library/react": "^14.0.0",
    "autoprefixer": "^10.4.14",
    "compression-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "globals": "^15.6.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.27",
    "postcss-assets": "^6.0.0",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^8.5.1",
    "react-refresh": "^0.14.0",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  }
}