{
  "name": "react-generator",
  "version": "1.0.0",
  "private": true,
  "description": "A React app generator bundled with Webpack 5 and Babel",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config webpack.dev.js",
    "build:local": "cross-env NODE_ENV=production webpack --config webpack.prod.js --env ANALYZE=true",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js --env ANALYZE=false",
    "test": "cross-env NODE_ENV=test jest --watchAll",
    "lint": "npx eslint src/**",
    "lint:fix": "npx eslint src/** --fix"
  },
  "dependencies": {
    "@chan_alston/image": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.5",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "compression-webpack-plugin": "^10.0.0",
    "core-js": "^3.30.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.39.0",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss": "^8.4.23",
    "postcss-assets": "^6.0.0",
    "postcss-loader": "^7.3.0",
    "postcss-preset-env": "^8.3.2",
    "react-refresh": "^0.14.0",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.2",
    "terser-webpack-plugin": "^5.3.7",
    "webpack": "^5.81.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3",
    "webpack-merge": "^5.8.0"
  }
}